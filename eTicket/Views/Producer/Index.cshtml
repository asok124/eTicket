@*definevidwmodel*@
@model IEnumerable<Producer>

@*define Viewdata*@

@*write code*@
<div class="row">
    <div class="col-md-8 offset-md-2">
        <table class="table">
            <thead>
                <tr>
                    <th>@Html.DisplayNameFor(model =>model.ProfilePicUrl)</th>
                    <th>@Html.DisplayNameFor(model =>model.Name)</th>
                    <th>@Html.DisplayNameFor(model =>model.bio)</th>
                    <th>Actions</th>

                </tr>

            </thead>
            <tbody>
                @foreach (var item in Model)
                {
                <td class="align-middle">
                    <img class="rounded-circle" src="@item.ProfilePicUrl " />
                </td>
                <td class="align-middle">@Html.DisplayFor(modelitem => item.Name)</td>
                <td class="align-middle">@Html.DisplayFor(modelitem => item.bio)</td>
                <td class="align-middle">
                    <a class="btn btn-outline-primary"> <i class="bi bi-pencil-square"></i> Edit </a>
                    <a class="btn btn-outline-info"> <i class="bi bi-pencil-eye"></i> Details </a>
                    <a class="btn btn-outline-danger"> <i class="bi bi-pencil-trash"></i> Delete </a>
                </td>
                }

            </tbody>

        </table>
    </div>
</div>